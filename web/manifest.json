{
    "name": "Deep Link Demo - Lightning Fast Flutter PWA",
    "short_name": "Deep Link Demo",
    "start_url": ".",
    "display": "standalone",
    "background_color": "#667eea",
    "theme_color": "#764ba2",
    "description": "Experience lightning-fast deep linking with our optimized Flutter PWA. Seamless navigation and instant loading across all devices.",
    "orientation": "portrait-primary",
    "categories": ["navigation", "utilities", "productivity", "developer-tools"],
    "lang": "en",
    "scope": "/",
    "id": "deep-link-demo-pwa-v2",
    "dir": "ltr",
    "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
    "icons": [
        {
            "src": "icons/Icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "icons/Icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "icons/Icon-maskable-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "icons/Icon-maskable-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "favicon.png",
            "sizes": "32x32",
            "type": "image/png",
            "purpose": "any"
        }
    ],
    "shortcuts": [
        {
            "name": "Home",
            "short_name": "Home",
            "description": "Go to home page with instant navigation",
            "url": "/home",
            "icons": [{ 
                "src": "icons/Icon-192.png", 
                "sizes": "192x192", 
                "type": "image/png" 
            }]
        },
        {
            "name": "Profile",
            "short_name": "Profile",
            "description": "View and manage your profile",
            "url": "/profile",
            "icons": [{ 
                "src": "icons/Icon-192.png", 
                "sizes": "192x192", 
                "type": "image/png" 
            }]
        },
        {
            "name": "Deep Link Test",
            "short_name": "Test",
            "description": "Test deep linking functionality",
            "url": "/deeplink?source=shortcut&test=true",
            "icons": [{ 
                "src": "icons/Icon-192.png", 
                "sizes": "192x192", 
                "type": "image/png" 
            }]
        },
        {
            "name": "Reports",
            "short_name": "Reports",
            "description": "View comprehensive reports and analytics",
            "url": "/report",
            "icons": [{ 
                "src": "icons/Icon-192.png", 
                "sizes": "192x192", 
                "type": "image/png" 
            }]
        }
    ],
    "screenshots": [
        {
            "src": "icons/Icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "form_factor": "wide",
            "label": "Deep Link Demo - Desktop View"
        },
        {
            "src": "icons/Icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "form_factor": "narrow",
            "label": "Deep Link Demo - Mobile View"
        }
    ],
    "prefer_related_applications": false,
    "edge_side_panel": {
        "preferred_width": 420
    },
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "protocol_handlers": [
        {
            "protocol": "web+deeplink",
            "url": "/deeplink?url=%s"
        }
    ],
    "share_target": {
        "action": "/deeplink",
        "method": "GET",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "file_handlers": [],
    "handle_links": "preferred",
    "scope_extensions": [
        {"origin": "*.example.com"}
    ]
}
